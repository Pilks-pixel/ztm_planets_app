{
  "name": "nasa-project-server",
  "version": "1.0.0",
  "description": "A server for NASA project API",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "start:cluster": "pm2 start dist/server.js -i max",
    "dev": "node --watch --experimental-strip-types src/server.ts",
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "format": "prettier --write \"**/*.+(.js|ts|json)\"",
    "lint": "eslint .",
    "validate": "npm run check-types && npm run lint && npm run format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "ts-jest-mock-import-meta": "^1.3.0",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.24.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.0",
    "morgan": "^1.10.0",
    "pm2": "^6.0.5"
  }
}
